{
  "version": "0.0.1",
  "private": true,
  "description": "A hipster Ionic project, made with ðŸ’™ by @jhipster!",
  "homepage": "https://www.jhipster.tech",
  "author": "Ionic Framework + JHipster",
  "scripts": {
    "build": "ionic build",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "prettier": "prettier --write \"{,e2e/**/,src/**/}*.{js,json,html,md,ts,css,scss,yml}\" --loglevel silent",
    "start": "ionic serve",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{js,ts}",
      "!src/app/**/index.ts",
      "!src/app/**/*.module.ts",
      "!<rootDir>/node_modules/"
    ],
    "coverageDirectory": "./public/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "preset": "jest-preset-angular",
    "resolver": "jest-preset-angular/build/resolvers/ng-jest-resolver.js",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!.*\\.mjs|$@ngrx|@ionic-native|@ionic)"
    ]
  },
  "dependencies": {
    "@angular/common": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@capacitor/app": "1.1.1",
    "@capacitor/core": "3.4.3",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/splash-screen": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@ionic-native/camera": "5.36.0",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/http": "5.36.0",
    "@ionic-native/secure-storage": "5.36.0",
    "@ionic-native/splash-screen": "5.36.0",
    "@ionic-native/status-bar": "5.36.0",
    "@ionic/angular": "^6.0.13",
    "@ionic/storage": "^3.0.6",
    "@ionic/storage-angular": "^3.0.6",
    "@ng-bootstrap/ng-bootstrap": "12.0.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "cordova-plugin-advanced-http": "3.2.2",
    "cordova-plugin-file": "6.0.2",
    "cordova-plugin-safariviewcontroller": "2.0.0",
    "cordova-plugin-secure-storage-echo": "5.1.1",
    "ionic-appauth": "0.8.5",
    "ngx-webstorage": "9.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/builder": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@capacitor/cli": "3.4.3",
    "@ionic/angular-toolkit": "^6.1.0",
    "@oktadev/schematics": "5.2.2",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/jest": "27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "38.0.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "html-loader": "3.1.0",
    "ionic-mocks-jest": "1.3.3",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "27.5.1",
    "jest-localstorage-mock": "2.4.19",
    "jest-preset-angular": "11.1.1",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "null-loader": "4.0.1",
    "prettier": "2.6.1",
    "protractor": "~7.0.0",
    "ts-loader": "9.2.8",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.3",
    "cypress": "latest"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there"
      }
    }
  }
}
