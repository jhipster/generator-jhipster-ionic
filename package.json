{
  "name": "generator-jhipster-ionic",
  "version": "8.1.0",
  "description": "A JHipster blueprint that generates an Ionic client.",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-8",
    "jhipster",
    "ionic"
  ],
  "homepage": "https://github.com/jhipster/generator-jhipster-ionic#readme",
  "bugs": {
    "url": "https://github.com/jhipster/generator-jhipster-ionic/issues"
  },
  "license": "Apache-2.0",
  "type": "module",
  "imports": {
    "#test-utils": "./test/utils.mjs"
  },
  "bin": {
    "jhipster-ionic": "cli/cli.cjs"
  },
  "files": [
    "cli",
    "generators",
    "!**/*.spec.{,m,c}js",
    "!**/__*"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "vitest run",
    "update-snapshot": "vitest run --update",
    "vitest": "vitest"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "generator-jhipster": "8.0.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "ejs-lint": "2.0.0",
    "eslint": "8.52.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-packagejson": "2.4.6",
    "vitest": "0.34.6",
    "yeoman-test": ">=8.0.0-rc.1"
  },
  "engines": {
    "node": "^18.13.0 || >= 20.6.1"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
